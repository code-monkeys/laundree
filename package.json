    "eslint-plugin-standard": "^3.0.1",
    "faker": "^4.1.0",
    "flow-bin": "^0.60.1",
    "flow-typed": "^2.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nightwatch": "^0.9.16",
    "nodemon": "^1.19.1",
    "nyc": "^11.9.0",
    "selenium-standalone": "^6.16.0",
    "sinon": "^4.0.1",
    "supertest": "^3.4.2",
